{
  "manifest_version": 2,
  "name": "FeedTheBeast-ModlistExtractor",
  "version": "1.0",

  "description": "Extract a list of all mods with or without project link.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.feed-the-beast.com/modpacks/*"],
      "js": ["sweetalert2-allmin.js", "extract.js"]
    }
  ]
}